!!!
%html
    %head
        %title donuts.io
        %link(href="ss/bootstrap.min.css" rel="stylesheet")
        %link(href="ss/navbar-fixed-top.css" rel="stylesheet")
        %link(href="ss/main.css" rel="stylesheet")
        %script(src="https://code.jquery.com/jquery-1.10.2.min.js")
        %script(src="js/bootstrap.min.js")
        %script(src="js/jquery.stellar.min.js")

        %script
            $(function(){
            $.stellar();
            });
    %body(data-spy="scroll" data-target=".navbar")
        %div(class="navbar navbar-inverse navbar-fixed-top" role="navigation")
            %div(class="container")
                %div(class="navbar-header")
                    %a(class="navbar-brand")eldonuts
                %div(class="navbar-collapse collapse")
                    %ul(class="nav navbar-nav")
                        %li(class="active")
                            %a(href="#cat1") cat1
                        %li
                            %a(href="#cat2") cat2
                        %li
                            %a(href="#cat3") cat3



        %div(class="row")
            %div(class= "cat1 sixhundred" id="cat1" data-stellar-background-ratio="0.5")
            %div(class= "cat2 sixhundred" id="cat2" data-stellar-background-ratio="0.5")
            %div(class= "cat3 sixhundred" id="cat3" data-stellar-background-ratio="1")
